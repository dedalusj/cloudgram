{
  "name": "cloudgram",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "test": "nyc jest",
    "lint": "node_modules/.bin/prettier --write \"{src,tests,scripts}/**/*.{js,ts}\"",
    "dev": "rm -fr dev && parcel src/index.html src/embed.html --no-cache --dist-dir dev",
    "build": "rm -fr dist && NODE_ENV=production parcel build src/index.html --no-cache --dist-dir dist",
    "check": "tsc --noEmit",
    "assets": "node --loader ts-node/esm --experimental-specifier-resolution=node scripts/assets.ts",
    "diagram": "node --loader ts-node/esm --experimental-specifier-resolution=node scripts/generate_grammar_diagram.ts",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "deploy": "scripts/deploy.sh",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "Jacopo Sabbatini",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@chevrotain/types": "^10.1.2",
    "@parcel/transformer-inline-string": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@types/ace": "^0.0.48",
    "@types/cytoscape": "^3.19.7",
    "@types/file-saver-es": "^2.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.2",
    "@types/nunjucks": "^3.2.1",
    "@types/tmp": "^0.2.3",
    "@types/xml2js": "^0.4.11",
    "axios": "^0.27.2",
    "babel-jest": "^28.1.3",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "glob-promise": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-transform-stub": "^2.0.0",
    "nunjucks": "^3.2.3",
    "nyc": "^15.1.0",
    "parcel": "^2.7.0",
    "pino": "^8.4.1",
    "pino-pretty": "^9.0.0",
    "postcss": "^8.4.16",
    "posthtml-include": "^1.7.4",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "tmp": "^0.2.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "unzipper": "^0.10.11",
    "url": "^0.11.0",
    "xml2js": "^0.4.23"
  },
  "dependencies": {
    "@sentry/browser": "^7.10.0",
    "@sentry/tracing": "^7.10.0",
    "ace-builds": "^1.9.5",
    "chevrotain": "^10.1.2",
    "cytoscape": "^3.22.1",
    "cytoscape-dagre": "^2.4.0",
    "cytoscape-svg": "^0.4.0",
    "file-saver-es": "^2.0.5"
  }
}
