{
  "name": "archdraw",
  "version": "0.0.1",
  "scripts": {
    "test": "NODE_ENV=test nyc jest",
    "lint": "node_modules/.bin/prettier --write \"{src,tests}/**/*.js\"",
    "coverage-badges": "jest-coverage-badges --output \"./badges\" && git add -A badges",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "assets": "scripts/assets.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "echo 'Pre-commit checks...' && exit 0",
      "pre-commit": "pretty-quick --staged && && npm test && npm run coverage-badges"
    }
  },
  "dependencies": {
    "ace-builds": "^1.4.12",
    "chevrotain": "^7.0.2",
    "cytoscape": "^3.16.2",
    "cytoscape-dagre": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@types/jest": "^26.0.14",
    "axios": "^0.20.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.5.2",
    "camelcase": "^6.1.0",
    "core-js": "^3.6.5",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-transform-stub": "^2.0.0",
    "mustache": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.4",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "regenerator-runtime": "^0.13.7",
    "serve": "^11.3.2",
    "unzipper": "^0.10.11"
  }
}
